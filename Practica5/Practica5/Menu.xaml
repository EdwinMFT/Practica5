<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Practica5.Menu">
  <!--Pages can be added as references or inline-->
    
    <ContentPage Title="Dispositivos Moviles">
        <StackLayout>
            <Button Text="Desarrollador" Clicked="Desarrollador_Clicked"/>
            
        </StackLayout>
    </ContentPage>

    <ContentPage Title="Capturar Datos">
        <StackLayout>
        <Button Text="Captura Tus Datos" Clicked="CapturarDatos_Clicked"/>
        </StackLayout>
    </ContentPage>
    <ContentPage Title="Calculadora">
        <StackLayout>
            <Button Text="Calculadora" Clicked="Calculadora_Clicked"></Button>
        </StackLayout>
    </ContentPage>
</TabbedPage>